<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.BorderPane?>
<StackPane fx:controller="sample.MenuControler"
                xmlns:fx="http://javafx.com/fxml" stylesheets="@Style.css" fx:id="menuPane">
    <ImageView StackPane.alignment="CENTER">
        <Image url="@Images/menuBackground.jpg"/>
    </ImageView>
    <ImageView fitWidth="600" fitHeight="236" fx:id="logo" translateY="180" StackPane.alignment="TOP_CENTER">
        <Image url="@Images/logo.png"/>
    </ImageView>
    <VBox fx:id="menuLabels" focusTraversable="true" onKeyPressed="#changeCursor" translateY="100" alignment="CENTER">
        <children>
            <Label styleClass="menuLabel" style="-fx-underline:true;">Nouvelle Partie</Label>
            <Label styleClass="menuLabel">Charger Partie</Label>
            <Label styleClass="menuLabel">Sauvgarder Partie</Label>
            <Label styleClass="menuLabel">Option</Label>
            <Label styleClass="menuLabel">Quiter</Label>
        </children>
    </VBox>

</StackPane>